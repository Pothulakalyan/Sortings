#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>ve(n,0);
    for(int i=0;i<n;i++){
        cin>>ve[i];
    }
    for(int i=0;i<n;i++){
        int min=ve[i],temp;
        int min_ind=i;
        for(int j=i+1;j<n;j++){
            if(min>ve[j]){
                min=ve[j];
                min_ind=j;
            }
        }
        temp=ve[i];
        ve[i]=ve[min_ind];
        ve[min_ind]=temp;
    }
    for(auto it:ve){
        cout<<it<<" ";
    }
}
